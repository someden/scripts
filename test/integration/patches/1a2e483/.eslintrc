{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"impliedStrict": true,
			"experimentalObjectRestSpread": true,
			"objectLiteralDuplicateProperties": false
		}
	},
	"env": {
		"es6": true,
		"browser": true
	}
}
